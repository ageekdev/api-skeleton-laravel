{
    "name": ":vendor_slug/:project_slug",
    "type": "project",
    "description": ":project_description - laravel v10.2.5.",
    "keywords": ["framework", "laravel", ":project_slug", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ageekdev/dev-login": "^2.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^10.10",
        "laravel/octane": "^1.3.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/vapor-cli": "^1.54",
        "laravel/vapor-core": "^2.28",
        "laravel/vapor-ui": "^1.7",
        "spiral/roadrunner": "^2.8.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.20",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.4",
        "nunomaduro/larastan": "^2.4",
        "pestphp/pest-plugin-laravel": "^1.4",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "analyse-clear": "vendor/bin/phpstan clear-result-cache",
        "format": "vendor/bin/pint",
        "check": [
            "@php artisan test",
            "@analyse-clear",
            "@analyse",
            "@format"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
